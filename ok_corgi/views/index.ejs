<% include ./partials/header %>

  <section id="dogpound" class="row">
    <div id="corgipic" class="col-md-6 col-md-offset-3 col-sm-10 col-sm-offset-1 main-corgi">
      <h2>Do you like <%= name %>?</h2>
      <img src="<%= photo %>" alt="<%= name %>" class="corgi-banner img-responsive">


      <form id="button1" name="likes" action="/dogs/<%= id %>" method="POST">
        <button class="btn btn-primary btn-lg" type="submit" name="like" value="false">Paw left</button>
      </form>
      <form id="button2" name="likes" action="/dogs/<%= id %>" method="POST">
        <button class="btn btn-primary btn-lg" type="submit" name="like" value="true">Paw right</button>
      </form>
    </div>

  </section>

  <section>
    <a class="btn btn-primary btn-lg" id="doglink" href="/liked_dogs">Dogs you've liked</a>
  </section>

  

<% include ./partials/footer %>
